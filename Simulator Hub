function Charge()
    while getgenv().AutoCharge == true do
        game:GetService("ReplicatedStorage").SkillRemote:FireServer("Charge",true,CFrame.new(1713.33594, 590.56958, 1379.12476, 0.925043046, 0.187800378, -0.330191374, 0, 0.869239807, 0.494390726, 0.379862249, -0.4573327, 0.804084241),CFrame.new(1720.88293, 577.769592, 1360.74634, 0.925086737, -2.97243385e-08, -0.379755855, 1.3494557e-11, 1, -7.82393599e-08, 0.379755855, 7.23730693e-08, 0.925086737),CFrame.new(1769.19421, 579.382446, 1339.83484, 0.575337172, 0.132246614, -0.807154298, -0, 0.986842155, 0.16168721, 0.817916393, -0.0930246636, 0.567766905))
        wait(2.5)
        game:GetService("ReplicatedStorage").SkillRemote:FireServer("Charge",false,CFrame.new(1713.33594, 590.56958, 1379.12476, 0.925043046, 0.187800378, -0.330191374, 0, 0.869239807, 0.494390726, 0.379862249, -0.4573327, 0.804084241),CFrame.new(1720.88293, 577.769592, 1360.74634, 0.925086737, -2.97243385e-08, -0.379755855, 1.3494557e-11, 1, -7.82393599e-08, 0.379755855, 7.23730693e-08, 0.925086737),CFrame.new(1769.19421, 579.382446, 1339.83484, 0.575337172, 0.132246614, -0.807154298, -0, 0.986842155, 0.16168721, 0.817916393, -0.0930246636, 0.567766905))
        wait(2)
    end
end

function Farm()
    spawn(function()
        while getgenv().AutoFarm == true do
            game:GetService("ReplicatedStorage").SkillRemote:FireServer("Combat",true,CFrame.new(1790.34766, 584.54187, 1333.14258, -0.9935956, -0.0476612411, 0.1024516, 0, 0.906689584, 0.421798646, -0.112995245, 0.419097275, -0.900882661),CFrame.new(1789.06702, 577.769409, 1344.40356, -0.947279871, -5.24419974e-09, 0.320407778, -4.10281142e-09, 1, 4.23737223e-09, -0.320407778, 2.69940625e-09, -0.947279871),CFrame.new(1783.34863, 574.771362, 1368.82483, -0.981293678, -0.0499642976, 0.185920417, -0, 0.965734661, 0.259531736, -0.192517102, 0.254676849, -0.947669208))
            wait()
            game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.StatsService.RebirthUp:FireServer()
            wait()
        end
    end)
end

local page2 = venyx:addPage("Misc", 5012544693)
local section2 = page2:addSection("Misc")

section2:addButton("anti afk", function()
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
       vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
       wait(1)
       vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
end)

section2:addButton("anti ban", function()
for i, v in pairs(AntiBanSystem) do
    v:Destroy()
    wait(1)
    end
end)

section2:addKeybind("Toggle Gui", Enum.KeyCode.RightShift, function()
print("Activated Keybind")
venyx:toggle()
end, function()
print("Changed Keybind")
end)


end
