                local number_2 = 0;
                local Target = game:GetService("ReplicatedStorage").Remotes.SurfaceCompleted;
                Target:FireServer(number_1, number_2);
                
                wait()
            end
                else
                    getgenv().automoney = false
        end
    end)
    
